<h1>Listing statics</h1>

<table>
  <tr>
    <th>Title</th>
    <th>Short url</th>
    <th>Content</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @statics.each do |static| %>
  <tr>
    <td><%= static.title %></td>
    <td><%= static.short_url %></td>
    <td><%= static.content %></td>
    <td><%= link_to 'Show', static %></td>
    <td><%= link_to 'Edit', edit_static_path(static) %></td>
    <td><%= link_to 'Destroy', static, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Static', new_static_path %>
