<div id="fb-root"></div>
<script>(function (d, s, id) {
    var js, fjs = d.getElementsByTagName(s)[0];
    if (d.getElementById(id)) return;
    js = d.createElement(s);
    js.id = id;
    js.src = "//connect.facebook.net/<%= t("social_tools.region") %>/all.js#xfbml=1&appId=282072331870664";
    fjs.parentNode.insertBefore(js, fjs);
}(document, 'script', 'facebook-jssdk'));
</script>

<div class="fb-like" data-href="<%= target_url %>" data-send="false" data-layout="button_count" data-width="89" data-show-faces="false"></div>